<h2>Users:</h2>
<table>
	<tr>
		<th>ID</th>		
		<th>Username</th>		
		<th>Age</th>		
		<th>Phone</th>		
	</tr>

	<%@users.each do |a|%>
		<tr>
			<td><%=a.id%></td>
			<td><%=a.username%></td>
			<td><%=a.age%></td>
			<td><%=a.phone%></td>
		</tr>	
	<%end%>

</table>
<%=link_to 'Create New User', new_user_path%>
